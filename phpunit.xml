<phpunit bootstrap="vendor/autoload.php">
    <testsuites>
        <testsuite name="Wisc">
            <directory>tests/ImpactOauthClient</directory>
        </testsuite>

    </testsuites>
    <filter>
        <whitelist>
            <directory suffix=".php">lib/ImpactOauthClient</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="coverage-html" target="./coverage/" lowUpperBound="35"
             highLowerBound="70"/>
    </logging>
</phpunit>
